{
  "name": "n8n-nodes-plain",
  "version": "0.1.0",
  "description": "n8n node for Plain API integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Plain Node Contributors",
    "email": "your-email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/n8n-nodes-plain.git"
  },
  "homepage": "https://github.com/your-username/n8n-nodes-plain#readme",
  "bugs": {
    "url": "https://github.com/your-username/n8n-nodes-plain/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r src/icons dist/icons && cp src/nodes/Plain/plain.svg dist/nodes/Plain/plain.svg",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Plain.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Plain/PlainNode.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "n8n-workflow": "^1.45.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.45.0"
  }
}